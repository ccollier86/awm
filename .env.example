# AWM Environment Configuration
# Copy to .env and fill in your values

# Required - Your Appwrite configuration
APPWRITE_PROJECT_ID=your-project-id
APPWRITE_ENDPOINT=http://localhost/v1
APPWRITE_API_KEY=your-api-key-if-using-server-sdk

# Optional - Override defaults
APPWRITE_DATABASE_ID=clinical-guidelines
AWM_SCHEMA=appwrite.schema
AWM_DEBUG=false
AWM_LOCK_OWNER=$(hostname)
